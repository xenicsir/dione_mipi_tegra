alias detect="v4l2-ctl -l"
alias stream1280="gst-launch-1.0 v4l2src device=/dev/video0 ! video/x-raw,format=BGRA,width=1280,height=1024 ! videoconvert ! video/x-raw,format=NV12 ! nvvidconv ! nvoverlaysink sync=false"
alias stream1024="gst-launch-1.0 v4l2src device=/dev/video0 ! video/x-raw,format=BGRA,width=1024,height=768 ! videoconvert ! video/x-raw,format=NV12 ! nvvidconv ! nvoverlaysink sync=false"
alias stream640="gst-launch-1.0 v4l2src device=/dev/video0 ! video/x-raw,format=BGRA,width=640,height=480 ! videoconvert ! video/x-raw,format=NV12 ! nvvidconv ! nvoverlaysink sync=false"
alias stream320="gst-launch-1.0 v4l2src device=/dev/video0 ! video/x-raw,format=BGRA,width=320,height=240 ! videoconvert ! video/x-raw,format=NV12 ! nvvidconv ! nvoverlaysink sync=false"
alias save1280="v4l2-ctl --set-fmt-video=width=1280,height=1024 --set-ctrl bypass_mode=0 --stream-mmap --stream-count=1 --stream-to="
alias save1024="v4l2-ctl --set-fmt-video=width=1024,height=768 --set-ctrl bypass_mode=0 --stream-mmap --stream-count=1 --stream-to="
alias save640="v4l2-ctl --set-fmt-video=width=640,height=480 --set-ctrl bypass_mode=0 --stream-mmap --stream-count=1 --stream-to="
alias save320="v4l2-ctl --set-fmt-video=width=320,height=240 --set-ctrl bypass_mode=0 --stream-mmap --stream-count=1 --stream-to="
